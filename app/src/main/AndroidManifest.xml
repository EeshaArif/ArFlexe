<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidsceneform">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permissoin.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.hardware.camera2.full" />

    <uses-feature
        android:name="android.hardware.camera.ar"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".augmentedimage.AugmentedImageActivity3"></activity>
        <activity android:name=".augmentedimage.AugmentedImageFragment2" />
        <activity android:name=".Desks" />
        <activity android:name=".Lamps" />
        <activity android:name=".Tables" />
        <activity android:name=".augmentedimage.AugmentedImageActivity" />
        <activity android:name=".augmentedimage.body_3d" />
        <activity android:name=".MainActivityG" />
        <activity android:name=".MainActivityE" />
        <activity android:name=".Chairs" />

        <meta-data
            android:name="com.google.ar.core"
            android:value="required" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>